{
    "name": "mediawiki/semantic-highcharts",
    "type": "mediawiki-extension",
    "description": "Adds the 'highcharts' result format to SMW",
    "license": "GPL-3.0+",
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "1.*,>=1.0.1",
        "highslide-software/highcharts": "3.0.9"
    },
    "authors": [
        {
            "name": "Kim Eik",
            "email": "kim@heldig.org",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "highslide-software/highcharts",
                "version": "3.0.9",
                "dist": {
                    "url": "http://code.highcharts.com/zips/Highcharts-3.0.9.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "autoload": {
        "files" : [
            "HighCharts.php"
        ],
        "classmap":[
            "src/"
        ]
    }
}
